    alias <LALT> = <kAlt>;
    alias <SPCE> = <kSp>;
    alias <ALGR> = <kAGr>;
    alias <LSGT> = <kLt>;
    alias <LEFT> = <kLft>;
    alias <HOME> = <kHom>;
    alias   <UP> = <kUp>;
    alias <PGUP> = <kPUp>;
    alias <DOWN> = <kDn>;
    alias <PGDN> = <kPDn>;
    alias <RGHT> = <kRgt>;
    alias  <END> = <kEnd>;
