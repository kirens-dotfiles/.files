xkb_keymap {
xkb_keycodes {
    minimum = 8;
    maximum = 255;

    // All keys are cammel case and prefixed with k as in key
    // The names reference the print on the keyboard
    // Only aliases should reference these to make remapping easier
    <kEsc> =   9;
    <kF1>  =  67;
    <kF2>  =  68;
    <kBL-> = 232;
    <kF3>  =  69;
    <kBL+> = 233;
    <kF4>  =  70;
    <kF5>  =  71;
    <kF6>  =  72;
    <kAPr> = 173;
    <kF7>  =  73;
    <kAPP> = 172;
    <kF8>  =  74;
    <kANe> = 171;
    <kF9>  =  75;
    <kVl-> = 122;
    <kF10> =  76;
    <kVl+> = 123;
    <kF11> =  95;
    <kMut> = 121;
    <kF12> =  96;
    <kPrt> = 107;
    <kIns> = 118;
    <kDel> = 119;
    <kPar> =  49;
    <k1>   =  10;
    <k2>   =  11;
    <k3>   =  12;
    <k4>   =  13;
    <k5>   =  14;
    <k6>   =  15;
    <k7>   =  16;
    <k8>   =  17;
    <k9>   =  18;
    <k0>   =  19;
    <kPls> =  20;
    <kAcc> =  21;
    <kBsp> =  22;
    <kTab> =  23;
    <kQ>   =  24;
    <kW>   =  25;
    <kE>   =  26;
    <kR>   =  27;
    <kT>   =  28;
    <kY>   =  29;
    <kU>   =  30;
    <kI>   =  31;
    <kO>   =  32;
    <kP>   =  33;
    <kARg> =  34;
    <kUml> =  35;
    <kApo> =  51;
    <kCap> =  66;
    <kA>   =  38;
    <kS>   =  39;
    <kD>   =  40;
    <kF>   =  41;
    <kG>   =  42;
    <kH>   =  43;
    <kJ>   =  44;
    <kK>   =  45;
    <kL>   =  46;
    <kOUm> =  47;
    <kAUm> =  48;
    <kRtn> =  36;
    <kLSh> =  50;
    <kZ>   =  52;
    <kX>   =  53;
    <kC>   =  54;
    <kV>   =  55;
    <kB>   =  56;
    <kN>   =  57;
    <kM>   =  58;
    <kCom> =  59;
    <kDot> =  60;
    <kDsh> =  61;
    <kRSh> =  62;
    <kAPs> = 127;
    <kLCt> =  37;
    <kLWn> = 133;
    <kAlt> =  64;
    <kSp>  =  65;
    <kAGr> = 108;
    <kMen> = 135;
    <kLt>  =  94;
    <kLft> = 113;
    <kHom> = 110;
    <kUp>  = 111;
    <kPUp> = 112;
    <kDn>  = 116;
    <kPDn> = 117;
    <kRgt> = 114;
    <kEnd> = 115;
    <kPwr> = 124;

    // Other unknown keys
    <LVL3> = 92;

    indicator 1 = "Caps Lock";
    indicator 2 = "Num Lock";
    indicator 3 = "Scroll Lock";
    indicator 4 = "Compose";
    indicator 5 = "Kana";
    indicator 6 = "Sleep";
    indicator 7 = "Suspend";
    indicator 8 = "Mute";
    indicator 9 = "Misc";
    indicator 10 = "Mail";
    indicator 11 = "Charging";
    virtual indicator 12 = "Shift Lock";
    virtual indicator 13 = "Group 2";
    virtual indicator 14 = "Mouse Keys";

    // Remappings
    alias <RTSH> = <kEsc>;
    alias <FK01> = <kF1>;
    alias <FK02> = <kF2>;
    alias <FK03> = <kF3>;
    alias <FK04> = <kF4>;
    alias <FK05> = <kF5>;
    alias <FK06> = <kF6>;
    alias <FK07> = <kF7>;
    alias <FK08> = <kF8>;
    alias <FK09> = <kF9>;
    alias <FK10> = <kF10>;
    alias <FK11> = <kF11>;
    alias <FK12> = <kF12>;
    alias <PRSC> = <kPrt>;
    alias  <INS> = <kIns>;
    alias <DELE> = <kDel>;
    alias <COMP> = <kMen>;
    alias <MENU> = <kMen>;
    alias <TLDE> = <kPar>;
    alias <Num1> = <k1>;
    alias <Num2> = <k2>;
    alias <Num3> = <k3>;
    alias <Num4> = <k4>;
